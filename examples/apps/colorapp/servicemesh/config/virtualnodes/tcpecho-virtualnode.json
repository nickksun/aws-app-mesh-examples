{
    "spec": {
        "listeners": [
            {
                "portMapping": {
                    "port": 2701,
                    "protocol": "tcp"
                },
                "healthCheck": {
                    "protocol": "tcp",
                    "healthyThreshold": 2,
                    "unhealthyThreshold": 2,
                    "timeoutMillis": 2000,
                    "intervalMillis": 5000
                }
            }
        ],
        "serviceDiscovery": {
            "dns": {
                "hostname": "tcpecho@@SERVICES_DOMAIN@@"
            }
        }
    },
    "virtualNodeName": "tcpecho-vn"
}
